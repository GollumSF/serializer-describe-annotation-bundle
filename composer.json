{
	"name" : "gollumsf/serializer-describe-annotation-bundle",
	"type" : "symfony-bundle",
	"description" : "Add class annotation for describe serializer property",
	"keywords" : [
		"Serialize",
		"Class",
		"Annotation",
		"Symfony"
	],
	"homepage" : "https://github.com/GollumSF/serializer-describe-annotation-bundle",
	"license" : "GPL-3.0-or-later",
	"authors" : [{
			"name" : "Damien Duboeuf",
			"email" : "smeagolworms4@gmail.com"
		}
	],
	"support" : {
		"email" : "smeagolworms4@gmail.com",
		"wiki" : "https://github.com/GollumSF/serializer-describe-annotation-bundle/blob/master/README.md"
	},
	"require" : {
        "php": ">=7.2",
		"symfony/framework-bundle": "^4.4|^5|^6",
		"symfony/serializer-pack": "^1",
		"doctrine/annotations": "^1|^2"
	},
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "nyholm/symfony-bundle-test": "1.8.1",
		"gollumsf/reflection-property-test": "^1.0"
    },
    "autoload" : {
        "psr-4" : {
            "GollumSF\\SerializerDescribeAnnotationBundle\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\GollumSF\\SerializerDescribeAnnotationBundle\\" : "tests/"
        }
    }
